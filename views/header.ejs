			transition: 0.3s;
		}
		
		.content-container-1{
			overflow: hidden;
			margin:10px 30px;
			position:relative;
			padding:20px 0px;
		}
		.content-container-1 .img-rotator-left{
			position:absolute;
			top: calc(50% - 15px);
			left: 1px;
		}
		.content-container-1 .img-rotator-right{
			position:absolute;
			top: calc(50% - 15px);
			right: 8px;
		}
		
		.content-container-1 img:hover,
		.content-container-1 img:hover{
			cursor: pointer;
		}

		.content-container-1 .c1-content{
			width:calc(((100vw - 60px) / 5) - 8px);
			border:1px solid #8A8383;
			background-color:#f1f1f1;
			overflow: hidden;
		}
		
		.content-container-1 .c1-content>img{
			width:240px;
			margin-left:6px;
		}

		@media screen and (max-height: 450px) {
		  .sidenav {padding-top: 15px;}
