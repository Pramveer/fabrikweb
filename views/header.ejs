	<script>
	  $( function() {
		  /*-------------------------------*/
		  function runEffect() {
		  var selectedEffect = "slide";

		  var options = {};
		  if ( selectedEffect === "scale" ) {
			options = { percent: 50 };
		  } else if ( selectedEffect === "size" ) {
			options = { to: { width: 200, height: 60 } };
		  }
		  $( "#txtSearchItem" ).toggle( selectedEffect, options, 500 );
		};

		$( "#imgSearchToggle" ).on( "click", function() {
		  runEffect();
		});
		/*-------------------------------*/
		$( ".banner-container a" ).on( "hover", function() {
			
		});		
		  
		$(".content-container-1").hover(function() {
				$(".img-rotator-left").removeClass("dis-none");
				$(".img-rotator-right").removeClass("dis-none");
		}, function() {
				$(".img-rotator-left").addClass("dis-none");
				$(".img-rotator-right").addClass("dis-none");
		});
		
		  function ToggleContentContainer(){
			  	if($("#c1-content-container-1").hasClass("dis-block")){					
					$("#c1-content-container-1").removeClass("dis-block");
					$("#c1-content-container-1").addClass("dis-none");
					$("#c1-content-container-2").removeClass("dis-none");
					$("#c1-content-container-2").addClass("dis-block");	
				}
				else if($("#c1-content-container-2").hasClass("dis-block")){
					$("#c1-content-container-2").removeClass("dis-block");
					$("#c1-content-container-2").addClass("dis-none");
					$("#c1-content-container-1").removeClass("dis-none");
					$("#c1-content-container-1").addClass("dis-block");
				}

		  }
			$(".img-rotator-left").click(function() {
				ToggleContentContainer();
			});		  
			$(".img-rotator-right").click(function() {
				ToggleContentContainer();
			});		  

	  });
  </script>
